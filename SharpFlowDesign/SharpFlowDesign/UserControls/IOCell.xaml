<UserControl x:Class="SharpFlowDesign.UserControls.IOCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SharpFlowDesign.UserControls"
            
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="300">
    <StackPanel Canvas.Top ="{Binding Position.Y}"  Orientation="Horizontal" IsHitTestVisible="True" ZIndex="100"  Thumb.DragDelta="onDragDelta" PreviewMouseDown="StackPanel_MouseLeftButtonUp">
        <local:Flow DataContext="{Binding Input}"></local:Flow>
        <local:FunctionUnit x:Name="FU" LabelContent="{Binding Name}"  ></local:FunctionUnit>
        <local:Flow DataContext="{Binding Output}"></local:Flow>
    </StackPanel>
</UserControl>
