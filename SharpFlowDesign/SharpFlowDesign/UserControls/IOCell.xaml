<UserControl x:Class="SharpFlowDesign.UserControls.IOCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SharpFlowDesign.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="300">
    <StackPanel Orientation="Horizontal" IsHitTestVisible="True" ZIndex="100"  Thumb.DragDelta="onDragDelta" MouseDown="StackPanel_MouseLeftButtonUp">
        <local:Flow></local:Flow>
        <local:FunctionUnit LabelContent="LabelContent"  ></local:FunctionUnit>
        <local:Flow></local:Flow>
    </StackPanel>
</UserControl>
