<?xml version="1.0" encoding="utf-8" ?>
<FlowDesign>
  
  <DataFlow>
    <Flow name="customer" to="Checkout"/>
    <Flow name="" from="Checkout"/>
  </DataFlow>

  <FunctionUnit name="Checkout">
    <DataFlow>
      <Flow name="customer" to="Is_Customer_has_credit"/>
      <Flow name=".HasCredit()" from="Is_Customer_has_credit" to="Checkout_with_creditcard"> </Flow>
      <Flow name=".HasNoCredit()" from="Is_Customer_has_credit" to="Checkout_with_cash"> </Flow>
      <Flow name="" from="Checkout_with_creditcard" to="outputConnection"/>
      <Flow name="" from="Checkout_with_cash" to="outputConnection"/>
      <Flow name="" from="outputConnection" />
     
    </DataFlow>

    <FunctionUnit name="Is_Customer_has_credit"/>
    <FunctionUnit name="Checkout_with_creditcard"/>
    <FunctionUnit name="Checkout_with_cash"/>
    
    <Connection name="outputConnection"/>
    
  </FunctionUnit>
  
  
</FlowDesign>
