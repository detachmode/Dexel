<?xml version="1.0" encoding="utf-8" ?>
<StreamViewModelDesign>
  
  <DataStreamViewModel>
    <StreamViewModel name="customer" to="Checkout"/>
    <StreamViewModel name="" from="Checkout"/>
  </DataStreamViewModel>

  <SoftwareCell name="Checkout">
    <DataStreamViewModel>
      <StreamViewModel name="customer" to="Is_Customer_has_credit"/>
      <StreamViewModel name=".HasCredit()" from="Is_Customer_has_credit" to="Checkout_with_creditcard"> </StreamViewModel>
      <StreamViewModel name=".HasNoCredit()" from="Is_Customer_has_credit" to="Checkout_with_cash"> </StreamViewModel>
      <StreamViewModel name="" from="Checkout_with_creditcard" to="outputConnectionTextBox"/>
      <StreamViewModel name="" from="Checkout_with_cash" to="outputConnectionTextBox"/>
      <StreamViewModel name="" from="outputConnectionTextBox" />
     
    </DataStreamViewModel>

    <SoftwareCell name="Is_Customer_has_credit"/>
    <SoftwareCell name="Checkout_with_creditcard"/>
    <SoftwareCell name="Checkout_with_cash"/>
    
    <ConnectionTextBox name="outputConnectionTextBox"/>
    
  </SoftwareCell>
  
  
</StreamViewModelDesign>
