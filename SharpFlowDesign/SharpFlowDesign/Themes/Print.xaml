<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SharpFlowDesign.Themes" xmlns:System="clr-namespace:System;assembly=mscorlib">

    <SolidColorBrush x:Key="BG" Color="White"/>
    <SolidColorBrush x:Key="GridColor" Color="White" />
    <SolidColorBrush x:Key="OuterFillPointerBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="FillPointerBrush" Color="DimGray"/>
