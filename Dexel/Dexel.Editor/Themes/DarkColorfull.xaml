<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:Dexel.Editor.CustomControls">

    <SolidColorBrush x:Key="BG" Color="#2d2d30"/>
    <SolidColorBrush x:Key="BGBrighter" Color="#3e3e40"/>
    <SolidColorBrush x:Key="BGDarker" Color="#252526"/>
    
    <SolidColorBrush x:Key="GridColor" Color="#353538" />
    <SolidColorBrush x:Key="BorderBrush" Color="#3e3e40" />
    <SolidColorBrush x:Key="OuterFillPointerBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="FillPointerBrush" Color="DeepSkyBlue"/>
    <SolidColorBrush x:Key="BorderSoftwareCellBrush" Color="DeepSkyBlue"/>
    <SolidColorBrush x:Key="FillSoftwareCellBrush" Color="#FF303030"/>
    <SolidColorBrush x:Key="Foreground" Color="White"/>
    <system:Double x:Key="StrokeThickness">3</system:Double>

</ResourceDictionary>